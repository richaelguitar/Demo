<widget id="io.cordova.ECordova" version="2.0.0" xmlns="http://www.w3.org/ns/widgets">

    <preference name="ErrorUrl" value="file:///android_asset/www/test.html" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="useBrowserHistory" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="showTitle" value="true" />
    <preference name="BackgroundColor" value="#F5F5F5" />
    <preference name="Fullscreen" value="false" />



    <!-- Wildcards are allowed for the protocol, as a prefix
         to the host, or as a suffix to the path -->
    <allow-navigation href="*" />





    <access href="*" />
    <!-- Enable requests to content: URLs -->
    <access origin="*" />


    <feature name="whitelistplugin">
     <param name="android-package" value="com.app.demo.plugin.WhitelistPlugin"/>
     <param name="onload" value="true"/>
    </feature>
</widget>
